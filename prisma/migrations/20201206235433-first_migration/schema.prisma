// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  bio           String?
  email         String      @unique
  id            Int         @default(autoincrement()) @id
  image         String?
  password      String?
  username      String?     @unique
}
